 <script>

    function slider(name,length) {
        let len =length;
    let prev = document.querySelector(`.${name} .carosoul-left` );
    let nxt = document.querySelector(`.${name} .carosoul-right` );
    let vidsec = document.querySelector(`.${name} .videos-section`);
    let current=0;
    
    nxt.addEventListener("click",(e)=>{
        console.log("hello");
        current=(current<len-3)?++current:0;
        console.log(current);
         vidsec.style.transform=`translateX(${-375.6*current}px)`;
       

    });
    prev.addEventListener("click",(e)=>{
       
        current=(current>len-2)?--current:len;
        console.log(current);
        vidsec.style.transform=`translateX(${-375.6*(current-3)}px)`;
    });

    }
   
 
